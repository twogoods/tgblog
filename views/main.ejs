	<div class="container">
		<div class="row">
			<div class="col-sm-3 userinfo">
				<a href="#" class="center-block">
					<img src=<%= userinfo.headurl %>  class="img-circle center-block"/>
				</a>
				<br>
				<p class="hidden" id="uid"><%= userinfo.uid %></p>
				<p class="center"><%= userinfo.name %></p>
				<p class="center"><%= userinfo.mark %></p>
				<div class="tag">
					<p class="center-block">
						<% for(var i=0;i<userinfo.tags.length;i++){ %>
						<% if(i%5==0){ %>
						<span class="label label-success"><%= userinfo.tags[i] %></span>
						<% }else if(i%5==1){ %>
						<span class="label label-primary"><%= userinfo.tags[i] %></span>
						<% }else if(i%5==2){ %>
						<span class="label label-info"><%= userinfo.tags[i] %></span>
						<% }else if(i%5==3){ %>
						<span class="label label-warning"><%= userinfo.tags[i] %></span>
						<% }else{ %>
						<span class="label label-danger"><%= userinfo.tags[i] %></span>
						<% } %>
						<% } %>
					</p>
				</div>
			</div>

