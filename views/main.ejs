				<div class="switch">
					<input type="checkbox" data-size="mini" data-on-text="阅读" data-off-text="编辑" id="switchmode" checked disabled />
				</div>
				<% if (have) { %>
				<% articles.forEach(function(article){ %>
				<div class="row">
					<div class="col-sm-12 blogtext" id="<%= article.pid %>">
